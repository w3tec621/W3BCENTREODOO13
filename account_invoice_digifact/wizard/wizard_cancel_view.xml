<?xml version="1.0"?>
<odoo>
	<data>

    	<record id="wizard_cancel_fel" model="ir.ui.view">
			<field name="name">wizard.cancel.fel.form.view</field>
			<field name="model">wizard.fel.cancel</field>
			<field name="arch" type="xml">
                <form string="Anulacion documento FEL">
                    <group>
                        <group>
                            <field name="invoice_id" readonly="1"/>
                        </group>
                    </group>
                    <field name="notes" nolabel="1" required="1" placeholder="Motivo de la anulacion"/>
                    <footer>
                        <button name="action_cancel" string="Anular" type="object" class="oe_highlight"/>
                        <button string="Cancelar" special="cancel" class="oe_link"/>
                    </footer>
                </form>
			</field>
		</record>

    </data>
</odoo>